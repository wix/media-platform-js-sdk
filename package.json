{
  "name": "media-platform-js-sdk",
  "version": "8.0.0-alpha.16",
  "description": "The official Wix Media Platform Javascript SDK",
  "author": "Wix Media Platform",
  "homepage": "https://github.com/wix/media-platform-js-sdk",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "browser",
    "server",
    "dist",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/media-platform-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wix/media-platform-js-sdk/issues"
  },
  "keywords": [
    "wix",
    "media",
    "platform",
    "image",
    "video",
    "audio",
    "file",
    "upload",
    "resize",
    "crop",
    "transcoding",
    "streaming"
  ],
  "private": false,
  "main": "./dist/src/index.js",
  "engines": {
    "node": ">= 6.0.0"
  },
  "scripts": {
    "build": "yoshi lint && yoshi build",
    "build:analyze": "yoshi build --analyze",
    "lint": "yoshi lint",
    "lint:fix": "yoshi lint --fix",
    "test": "yoshi test --mocha --karma --protractor",
    "test:karma": "yoshi test --karma",
    "test:mocha": "yoshi test --mocha",
    "documentation": "okidoc-md ./docs/docs.yml ./docs",
    "documentation:gitadd": "npm run documentation && git add ./docs",
    "documentation:site:develop": "okidoc-site develop ./docs/site.yml",
    "documentation:site:build": "npm run documentation && okidoc-site build ./docs/site.yml",
    "documentation:deploy": "gh-pages -d ./sitedist",
    "standard-release": "standard-version --prerelease alpha",
    "standard-release:alpha": "standard-version --prerelease alpha",
    "standard-release:beta": "standard-version --prerelease beta",
    "standard-release:patch": "standard-version --release-as patch",
    "standard-release:minor": "standard-version --release-as minor",
    "standard-release:major": "standard-version --release-as major",
    "start": "yoshi start",
    "start-light": "yoshi start --no-test",
    "start:demo": "NODE_ENV=production npm run build && ts-node ./demo/bin/www",
    "e2e": "yoshi test --protractor",
    "karma": "yoshi test --karma",
    "travis-script": "npm run travis-test",
    "travis-test": "npm run travis-test:mocha",
    "travis-test:mocha": "mocha --require ts-node/register test/mocha-setup.ts {test,src}/**/*.spec.ts --no-timeouts",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@types/async": "^2.4.1",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/zen-observable": "^0.8.0",
    "async": "^2.6.2",
    "core-decorators": "^0.20.0",
    "eventemitter3": "^2.0.2",
    "inherits": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "zen-observable": "^0.8.14",
    "snyk": "^1.231.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.6",
    "@types/nock": "^9.3.1",
    "@types/node": "^9.6.47",
    "@types/sinon": "^5.0.7",
    "@types/sinon-chai": "^2.7.35",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-wix": "^1.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "conventional-changelog-cli": "^2.0.12",
    "cookie-parser": "^1.4.4",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "faux-jax": "latest",
    "file-api": "latest",
    "gh-pages": "^1.2.0",
    "husky": "^1.3.1",
    "jasmine-ajax": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jsdom": "^10.1.0",
    "jsdom-global": "^3.0.2",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "latest",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-ajax": "^0.1.13",
    "karma-mocha": "latest",
    "karma-phantomjs-launcher": "latest",
    "karma-teamcity-reporter": "^1.1.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "okidoc": "^2.0.0-beta.5",
    "prettier": "^1.16.4",
    "prismjs": "^1.16.0",
    "pug": "latest",
    "puppeteer": "^1.14.0",
    "request-debug": "latest",
    "semver": "^5.7.0",
    "sinon": "^4.3.0",
    "sinon-chai": "^3.3.0",
    "standard-version": "^4.4.0",
    "ts-node": "^5.0.0",
    "tslint-config-wix": "^1.0.31",
    "tslint-config-yoshi": "^3.30.5",
    "typescript": "^3.4.3",
    "uuid": "^3.3.2",
    "wnpm-ci": "^7.0.34",
    "yoshi": "^3.31.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "unpkg": true,
  "yoshi": {
    "entry": {
      "media-platform": "browser"
    },
    "exports": "MP",
    "specs": {
      "browser": "test/{mocha-setup.ts,browser/**/*.test.ts}"
    },
    "servers": {
      "cdn": {
        "port": 3211
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yoshi lint"
    }
  },
  "snyk": true
}
