{
  "name": "media-platform-js-sdk",
  "version": "6.0.0-alpha",
  "description": "The official Wix Media Platform Javascript SDK",
  "author": "Wix Media Platform",
  "homepage": "https://github.com/wix/media-platform-js-sdk",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "src",
    "public",
    "README.md",
    "LICENSE.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/media-platform-js-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wix/media-platform-js-sdk/issues"
  },
  "keywords": [
    "wix",
    "media",
    "platform",
    "image",
    "video",
    "audio",
    "file",
    "upload",
    "resize",
    "crop",
    "transcoding",
    "streaming"
  ],
  "private": false,
  "main": "./src/main.js",
  "browser": "./src/index.js",
  "engines": {
    "node": ">= 4.0.0"
  },
  "haste": {
    "preset": "yoshi"
  },
  "scripts": {
    "build": "yoshi build",
    "lint": "yoshi lint",
    "test": "yoshi test --mocha --karma --protractor",
    "release": "#yoshi release",
    "start": "yoshi start",
    "e2e": "yoshi test --protractor",
    "karma": "yoshi test --karma",
    "travis-script": "./scripts/travis-script.sh",
    "travis-test": "npm run travis-test:mocha",
    "travis-test:mocha": "mocha --require ts-node/register **/*.spec.ts --no-timeouts"
  },
  "dependencies": {
    "async": "^2.1.4",
    "core-decorators": "^0.20.0",
    "eventemitter3": "^2.0.2",
    "inherits": "^2.0.3",
    "jsonwebtoken": "^8.0.0",
    "request": "^2.81.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/chai": "~4.0.0",
    "@types/mocha": "^2.2.48",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-wix": "^1.0.3",
    "body-parser": "latest",
    "chai": "^4.1.2",
    "cookie-parser": "latest",
    "express": "latest",
    "faux-jax": "latest",
    "file-api": "latest",
    "haste-preset-yoshi": "^1.0.45",
    "husky": "~0.14.0",
    "jsdom": "^10.1.0",
    "jsdom-global": "^3.0.2",
    "karma": "1.4.0",
    "karma-chrome-launcher": "latest",
    "karma-mocha": "latest",
    "karma-phantomjs-launcher": "latest",
    "mocha": "latest",
    "nock": "9.0.2",
    "prismjs": "^1.6.0",
    "pug": "latest",
    "puppeteer": "^1.1.0",
    "request-debug": "latest",
    "sinon": "^4.3.0",
    "sinon-chai": "^2.14.0",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.1",
    "uuid": "^3.0.1",
    "wnpm-ci": "latest"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy"
    ],
    "presets": [
      "wix"
    ]
  },
  "unpkg": true,
  "yoshi": {
    "specs": {
      "browser": "test/browser/**/*.test.ts"
    }
  }
}
